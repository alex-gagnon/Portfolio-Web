{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "agagnon227/portfolio",
            "essential": true,
            "memory": 128,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 5000
                }
            ]
        },
        {
            "name": "server",
            "image": "agagnon227/portfolio-api",
            "hostname": "api",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "agagnon227/nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "links": ["client", "server"],
            "memory": 128
        }
    ]
  }